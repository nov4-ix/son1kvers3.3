# Pixel Learning Environment Variables

# Feature Flags
VITE_ENABLE_PIXEL_LEARNING=true
VITE_ENABLE_PIXEL_ML=false

# Optional Supabase Integration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Performance Settings
VITE_PIXEL_COUNT=10000
VITE_ADAPTATION_RADIUS=100
VITE_GRID_SIZE=50

# ML Settings (when enabled)
VITE_ML_MODEL_PATH=localstorage://pixel-model
VITE_ML_TRAINING_EPOCHS=50
VITE_ML_BATCH_SIZE=32

# Debug Settings
VITE_DEBUG_MODE=false
VITE_SHOW_FPS=false
VITE_LOG_INTERACTIONS=false
